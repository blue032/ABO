<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"> <!-- 모양을 직사각형으로 설정 -->
    <solid android:color="#FFFFFF"/> <!-- 배경색은 흰색 -->
    <stroke android:width="0.7dp" android:color="#000000"/> <!-- 테두리는 검정색, 두께 1dp -->
    <corners android:radius="4dp"/> <!-- 모서리를 둥글게, 반지름 4dp -->
</shape>
